ton ","C:\\Users\\Glincky\\vigma\\Front-End Build\\vigma_build\\src\\components\\order\\OrderContent.js",["292"],"import OrdersFooter from \"./OrdersFooter\";\r\nimport AntiButton from \"../ui/AntiButton\";\r\nimport Question from \"../ui/Question\";\r\nimport Content from \"../layout/Content\";\r\nimport Orders from \"./Orders\";\r\nimport Link from \"next/link\";\r\nimport classes from \"./OrderContent.module.css\";\r\n\r\nconst OrderContent = (props) => {\r\n  const { question, ...orderContent } = props;\r\n\r\n  return (\r\n    <Content className={classes.OrderContent}>\r\n      <Question>{props.question}</Question>\r\n      <Orders orders={props.orders} />\r\n      {!props.title ? (\r\n        <Link href=\"/contacts\">\r\n          <AntiButton>По Телефону</AntiButton>\r\n        </Link>\r\n      ) : (\r\n        <OrdersFooter>{props.title}</OrdersFooter>\r\n      )}\r\n    </Content>\r\n  );\r\n};\r\n\r\nexport default OrderContent;\r\n","C:\\Users\\Glincky\\vigma\\Front-End Build\\vigma_build\\src\\components\\order\\Orders.js",["293"],"import OrderSection from './OrderSection'\r\nimport classes from './Orders.module.css'\r\n\r\nconst Orders = (props) => {\r\n  return (\r\n    <div className={classes.Orders}>\r\n      {props.orders.map((order) => {\r\n        return <OrderSection {...order} key={Math.random()} />;\r\n      })}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Orders;\r\n","C:\\Users\\Glincky\\vigma\\Front-End Build\\vigma_build\\src\\components\\order\\OrderSection.js",["294"],"import { useRouter } from 'next/router'\r\nimport Secondary from \"../ui/Secondary\";\r\nimport Section from \"../ui/Section\";\r\nimport classes from \"./OrderSection.module.css\";\r\n\r\nconst OrderSection = (props) => {\r\n  const { imgSrc, others, total, query, title, ...sectionProps } = props;\r\n  const router = useRouter();\r\n\r\n  const formattedQuery = query.slice(1).split(\"=\");\r\n  const searchParams = new URLSearchParams(router.query);\r\n  searchParams.delete(formattedQuery[0]);\r\n  searchParams.append(...formattedQuery);\r\n\r\n  let queryUrl = `${router.pathname}?${searchParams}`;\r\n  if (total) queryUrl = `${`/order?${searchParams}`}`\r\n\r\n  const orderClickHandler = (e) => {\r\n    router.push(queryUrl);\r\n  };\r\n\r\n  return (\r\n    <Section onClick={orderClickHandler} className={classes.OrderSection}>\r\n      <img src={imgSrc} />\r\n      <div className={classes.Title}>\r\n        <Secondary>{others ? \"Другое (не знаю)\" : title}</Secondary>\r\n      </div>\r\n    </Section>\r\n  );\r\n};\r\n\r\nexport default OrderSection;\r\n","C:\\Users\\Glincky\\vigma\\Front-End Build\\vigma_build\\src\\components\\order\\OrdersFooter.js",["295"],"import { useRouter } from \"next/router\";\r\nconst arrowSvg = \"/order-assets/next-arrow.svg\";\r\nimport Third from \"../ui/Third\";\r\nimport classes from \"./OrdersFooter.module.css\";\r\n\r\nconst OrdersFooter = (props) => {\r\n  const { children, query, ...footerProps } = props;\r\n  const router = useRouter();\r\n\r\n  const clickHandler = (e) => {\r\n    const searchParams = new URLSearchParams(router.query);\r\n    let lastEntry = \"\";\r\n    searchParams.forEach((value, key) => (lastEntry = key));\r\n    searchParams.delete(lastEntry)\r\n\r\n    router.push(`${location.pathname}?${searchParams}`);\r\n  };\r\n\r\n  return (\r\n    <div className={classes.OrdersFooter}>\r\n      <img\r\n        onClick={clickHandler}\r\n        src={arrowSvg}\r\n        alt=\"Назад\"\r\n        className={classes.Arrow}\r\n      />\r\n      <Third>{children}</Third>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default OrdersFooter;\r\n","C:\\Users\\Glincky\\vigma\\Front-End Build\\vigma_build\\src\\components\\reviews\\Rating.js",["296"],"const starSrc = \"/reviews-assets/star.svg\";\r\nimport classes from \"./Rating.module.css\";\r\n\r\nconst Rating = (props) => {\r\n  const rating = new Array(props.rating).fill(true);\r\n  const nonActive = new Array(5 - props.rating).fill(false);\r\n\r\n  return (\r\n    <div className={classes.Rating}>\r\n      {rating.map((star) => {\r\n        return (\r\n          <img\r\n            className={classes.Star}\r\n            src={starSrc}\r\n            key={Math.random()}\r\n            alt=\"Звезда\"\r\n          />\r\n        );\r\n      })}\r\n      {nonActive.map((star) => {\r\n        return (\r\n          <img\r\n            className={`${classes.Star} ${classes.NonActive}`}\r\n            key={Math.random()}\r\n            src={starSrc}\r\n            alt=\"Звезда\"\r\n          />\r\n        );\r\n      })}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Rating;\r\n","C:\\Users\\Glincky\\vigma\\Front-End Build\\vigma_build\\src\\components\\reviews\\Review.js",["297"],"import Secondary from '../ui/Secondary'\r\nimport Section from '../ui/Section'\r\nimport Rating from './Rating'\r\nimport classes from './Review.module.css'\r\n\r\nconst Review = props => {\r\n  const {\r\n    imageSrc,\r\n    rating,\r\n    name,\r\n    text,\r\n    ...reviewProps\r\n  } = props\r\n\r\n  return (\r\n    <Section \r\n      {...reviewProps} \r\n      className={classes.Review}\r\n    >\r\n      <img src={imageSrc} alt={name} className={classes.Image} />     \r\n      <Rating rating={rating} />\r\n      <Secondary className={classes.Name}>{name}</Secondary>\r\n      <p className={classes.Text}>{text}</p>\r\n    </Section> \r\n  )\r\n}\r\n\r\nexport default Review","C:\\Users\\Glincky\\vigma\\Front-End Build\\vigma_build\\src\\components\\reviews\\Reviews.js",["298"],"const woman = \"/reviews-assets/woman in a blue shirt.jpg\";\r\nconst coolMan = \"/reviews-assets/cool man.jpg\";\r\nconst coolMan2 = \"/reviews-assets/cool man 2.jpg\";\r\nconst oldMan = \"reviews-assets/old man.jpg\";\r\n\r\nimport Review from \"./Review\";\r\nimport classes from \"./Reviews.module.css\";\r\n\r\nconst reviews = [\r\n  {\r\n    imageSrc: woman,\r\n    name: \"Ольга Рудц\",\r\n    rating: 5,\r\n    text: \"Изготовили и привезли всё в оговоренные сроки, окна и подоконник у нас стандартного белого цвета, как я поняла, цветные делают дольше. Единственный косяк, вместе с окнами и материалами в день... доставки не довезли мешок с редмондом, позвонили извинились, пришлось на следующий день идти еще принимать его. Установка прошла очень чётко и организовано – в первый рабочий день после майских праздников поставили окно и балконную дверь, порадовало что всё сделали очень аккуратно и не особо грязно, даже прибрали за собой. Устанавливать откосы мастера были готовы уже на следующий день, но у нас планировались розетки в откосах и мы зависели от электрика, поэтому дальнейшая установка продолжилась дней через 10. Монтажники все были русские, адекватные. Всё делали очень тщательно, только откосы устанавливали около 5 часов. Никаких доплат, всё как прописано в договоре.\",\r\n  },\r\n  {\r\n    imageSrc: coolMan,\r\n    name: \"Николай Стенгов\",\r\n    rating: 4,\r\n    text: \"Качество кухни очень хорошее — ровный, однотонный профиль, отличная фурнитура.  Отдельное спасибо установщику Михаилу, который сумел быстро установить полки...  Кухня приобрела новый, \",\r\n  },\r\n  {\r\n    imageSrc: coolMan2,\r\n    name: \"Максим Мидний\",\r\n    rating: 5,\r\n    text: \"Из положительного: внимательное и добожелательное отношение на всех этапах и у всех сотрудников; конструктивное и оперативное решение с недостающим окном - сделали скидку (процесс решения...\",\r\n  },\r\n  {\r\n    imageSrc: oldMan,\r\n    name: \"Николай Стровский\",\r\n    rating: 5,\r\n    text: \"Сделали, доставили и установили новую качественну кровать очень быстро. Спасибо!\",\r\n  },\r\n];\r\n\r\nconst Reviews = (props) => {\r\n  return (\r\n    <article className={classes.Reviews}>\r\n      {reviews.map((review) => {\r\n        return <Review {...review} key={Math.random()} />;\r\n      })}\r\n    </article>\r\n  );\r\n};\r\n\r\nexport default Reviews;\r\n","C:\\Users\\Glincky\\vigma\\Front-End Build\\vigma_build\\src\\components\\tot